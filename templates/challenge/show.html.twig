<!DOCTYPE html>

<title>Challenge</title>

{% block body %}
    <h1>Challenge</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ challenge.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ challenge.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ challenge.description }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ challenge.dateDebut ? challenge.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ challenge.dateFin ? challenge.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ challenge.image }}</td>
            </tr>
            <tr>
                <th>FichierCahierCharges</th>
                <td>{{ challenge.fichierCahierCharges }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_challenge_index') }}">back to list</a>

    <a href="{{ path('app_challenge_edit', {'id': challenge.id}) }}">edit</a>

    {{ include('challenge/_delete_form.html.twig') }}
{% endblock %}
